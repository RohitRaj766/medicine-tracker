{
  "expo": {
    "name": "sanjeevaniAI",
    "slug": "sanjeevaniAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.jpg",
    "scheme": "sanjeevaniAI",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/images/logo.jpg",
        "backgroundImage": "./src/assets/images/logo.jpg",
        "monochromeImage": "./src/assets/images/logo.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rohitraj2k04.sanjeevaniAI"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/src/images/logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aada09fd-5295-4c6e-8c9a-293ce9dae728"
      }
    }
  }
}
